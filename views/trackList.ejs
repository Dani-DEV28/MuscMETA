<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>MusicMETA</title>
    <link rel="icon" type="image/jpg" href="img/CD-image.jpg">
    <link rel="stylesheet" href="/styles/style.css">
</head>
<body>
    <div id = headerTitle>
        <form action="/" method="post">
            <button type="submit" id = navi>ðŸ”™ Go Home</button>
        </form>
        <H2>Album Track List</H2>
        <button onclick="history.go(-1)" id = navi>ðŸ”™ Go Back</button>
    </div>
    <!-- <p>Check console log for TrackList</p> -->
    <div id="trackInfo">
        <img src="<%= imgPath %>" id="trackImg" alt="album cover">
        <% if (tracks && tracks.length > 0) { %>
          <div class="track-list">
              <p>Total Tracks: <%= album.TrackCount %></p>
              <table class="track-table">
                  <thead>
                      <tr>
                          <th>Track #</th>
                          <th>Track Name</th>
                          <th>Duration</th>
                      </tr>
                  </thead>
                  <tbody>
                      <% tracks.forEach(track => { %>
                          <tr>
                              <td><%= track.TrackNum %></td>
                              <td><%= track.TrackName %></td>
                              <td><%= track.track_length %></td>
                          </tr>
                      <% }); %>
                  </tbody>
              </table>
          </div>
      <% } else { %>
          <p class="no-tracks">No tracks found for this album.</p>
      <% } %>
  
    </div>

</body>
</html>

